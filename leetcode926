class Solution {
public:
    int minFlipsMonoIncr(string s) {
        int count_oneS = 0;
        int flipS = 0;

        for(char &ch : s){
            if(ch == '1'){
                count_oneS++;
            }else{
                flipS = min(flipS+1, count_oneS);
            }
        }
        return flipS;
    }
};





